<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GradientButton">
        <attr name="button_text" format="string" />
        <attr name="button_size" format="dimension" />
        <attr name="button_text_color" format="color" />
        <attr name="button_start_color" format="color" />
        <attr name="button_end_color" format="color" />
        <attr name="button_press_start_color" format="color" />
        <attr name="button_press_end_color" format="color" />
        <attr name="button_gradient_orientation" format="enum">
            <enum name="TOP_BOTTOM" value="0" />
            <enum name="TR_BL" value="1" />
            <enum name="RIGHT_LEFT" value="2" />
            <enum name="BR_TL" value="3" />
            <enum name="BOTTOM_TOP" value="4" />
            <enum name="BL_TR" value="5" />
            <enum name="LEFT_RIGHT" value="6" />
            <enum name="TL_BR" value="7" />
        </attr>
        <attr name="button_radius" format="dimension" />
        <attr name="button_shadow_distance" format="dimension" />
        <attr name="button_shadow_angle" format="integer" />
        <attr name="button_shadow_color" format="color" />
        <attr name="button_is_shadowed" format="boolean" />
        <attr name="button_shadow_radius" format="dimension" />
        <attr name="button_padding" format="dimension" />
        <attr name="button_padding_start" format="dimension" />
        <attr name="button_padding_top" format="dimension" />
        <attr name="button_padding_end" format="dimension" />
        <attr name="button_padding_bottom" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="round_color" format="color" />
        <attr name="round_progressColor" format="color" />
        <attr name="round_width" format="dimension" />
        <attr name="round_textColor" format="color" />
        <attr name="round_textsize" format="dimension" />
        <attr name="round_max" format="integer" />
    </declare-styleable>

    <declare-styleable name="SmileView">
        <attr name="smil_radius" format="integer" />
        <attr name="smil_line_width" format="integer" />
        <attr name="smil_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="PullZoomView">
        <attr name="pzv_sensitive" format="float" />
        <attr name="pzv_isZoomEnable" format="boolean" />
        <attr name="pzv_isParallax" format="boolean" />
        <attr name="pzv_zoomTime" format="integer" />
    </declare-styleable>

    <declare-styleable name="DetailLayout">
        <attr name="left_text" format="string" />
        <attr name="right_text" format="string" />
        <attr name="right_text_maxEms" format="integer" />
        <attr name="right_text_maxLines" format="integer" />
        <attr name="right_text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="FillImageView">
        <attr name="checkImage" format="reference" />
        <attr name="unCheckImage" format="reference" />
    </declare-styleable>

    <declare-styleable name="DrawableTintTextView">
        <attr name="drawable_tint_color" format="color" />
    </declare-styleable>

    <!--    <declare-styleable name="RecommendHeader">-->
    <!--        <attr name="header_title" format="string" />-->
    <!--    </declare-styleable>-->

    <declare-styleable name="RecommendCard">
        <attr name="header_title" format="string" />
    </declare-styleable>

    <declare-styleable name="PercentImageView">
        <attr name="piv_basics" format="enum">
            <enum name="width" value="0" />
            <enum name="height" value="1" />
        </attr>
        <attr name="piv_percent" format="float" />
    </declare-styleable>

    <declare-styleable name="SubmitView">
        <attr name="sv_bindEditText1" format="reference" />
        <attr name="sv_bindEditText2" format="reference" />
        <attr name="sv_bindEditText3" format="reference" />
        <attr name="sv_bindEditText4" format="reference" />
    </declare-styleable>

    <declare-styleable name="ParticleView">
        <attr name="pv_count" format="integer" />
        <attr name="pv_alpha" format="integer" />
        <attr name="pv_min_size" format="integer" />
        <attr name="pv_max_size" format="integer" />
        <attr name="pv_color" format="color" />
        <attr name="pv_background" format="reference" />
    </declare-styleable>






    <string-array name="default_wallpaper_tags">
        <item>全部</item>
        <item>壁纸</item>
        <item>二次元</item>
        <item>美女</item>
        <item>Cosplay</item>
        <item>生活</item>
        <item>萌宠</item>
        <item>艺术</item>
        <item>小清新</item>
        <item>运动</item>
        <item>游戏</item>
        <item>风景</item>
        <item>动漫</item>
        <item>明星</item>
        <item>美食</item>
        <item>酷车</item>
    </string-array>

</resources>